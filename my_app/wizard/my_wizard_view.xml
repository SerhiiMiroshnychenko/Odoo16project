<odoo>
    <data>

        <record id="my_wizard_form_view" model="ir.ui.view">
            <field name="name">my.wizard.form</field>
            <field name="model">my.wizard</field>
            <field name="arch" type="xml">
                <form string="My Wizard">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="apply_change_name" type="object" string="Apply" class="oe_highlight"/>
                        <button string="Cancel" special="cancel" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>

